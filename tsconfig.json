{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ES2024",
		"jsx": "react-jsx",
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strict": true,
		"sourceMap": true,
		"paths": {
			"@shared/*": ["./Cliente/src/shared/*"]
		}
	},
	"exclude": ["node_modules", "**/node_modules/*"]
}
